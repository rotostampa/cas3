# JWT secret for token validation
JWT_SECRET=your_jwt_secret_here

# S3 bucket name for storing files
S3_BUCKET=your-cas3-bucket

# Server bind address (optional, defaults to 0.0.0.0:3000)
BIND_ADDR=0.0.0.0:3000

# AWS credentials (if not using IAM roles or other AWS credential providers)
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-east-1

# Optional: AWS session token (for temporary credentials)
# AWS_SESSION_TOKEN=your_session_token

# Optional: AWS endpoint URL (for S3-compatible services like MinIO)
# AWS_ENDPOINT_URL=http://localhost:9000
